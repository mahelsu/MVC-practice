<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @*<link rel="icon" href="images/favicon.ico" type="image/ico" />*@

    <title> @ViewBag.Tittle Huascar Mejía </title>
    <!-- Bootstrap -->
    <link href="~/Vendors/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Vendors/fontawesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- NProgress -->
    <link href="~/Vendors/nprogress/nprogress.css" rel="stylesheet" />
    <!-- Custom Theme Style -->
    <link href="~/Content/Alela/css/custom.min.css" rel="stylesheet" />
    <link href="~/Content/Alela/css/custom.css" rel="stylesheet" />

    <link href="~/Content/myCss/mCss.css" rel="stylesheet" />
    <link href="~/Content/CreativeButtons/css/default.css" rel="stylesheet" />
    <link href="~/Content/CreativeButtons/css/component.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Raleway:100,300,400,500,700,900" rel="stylesheet">



</head>
<body>
    <div class="nav-md">
        <div class="container body">
            <div class="main_container">
                <div class="col-md-3 left_col">
                    <div class="left_col scroll-view">
                        <div class="navbar nav_title" style="border: 0;">
                            <a href="/Home/Index" class="site_title"> <span>Doc Control</span></a>
                        </div>

                        <div class="clearfix"></div>

                        <!-- menu profile quick info -->
                        <div class="profile clearfix">
                            <div class="profile_pic">

                            </div>
                            <div class="profile_info">
                                <span>Desarrollado por: </span>
                                <h2>Huascar Mejia</h2>
                            </div>
                        </div>
                        <!-- /menu profile quick info -->

                        <br />

                        <!-- sidebar menu -->
                        <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                            <div class="menu_section">
                                <h3>General</h3>
                                <ul class="nav side-menu">
                                    <li>
                                        <a href="/Home/Index"><i class="fa fa-home"></i> Home </a>

                                    </li>
                                    <li>
                                        <a href="/Home/FormControl"><i class="fa fa-edit"></i> Forms </a>

                                    </li>

                                    <li>
                                        <a href="/Home/ControlRegistros"><i class="fa fa-table"></i> Control </a>

                                    </li>
                                    <li>
                                        <a href="/Report/IndexReport"><i class="fa fa-bar-chart-o"></i> Reportes </a>

                                    </li>

                                </ul>
                            </div>
                        </div>

                        <!-- /menu footer buttons -->
                        @*<div class="sidebar-footer hidden-small">
                            <a data-toggle="tooltip" data-placement="top" title="Settings">
                                <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                            </a>

                            <a data-toggle="tooltip" data-placement="top" title="Lock">
                                <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                            </a>

                        </div>*@
                        <!-- /menu footer buttons -->
                    </div>
                </div>
                  @*<div id="social-icons">
                    <div class="text-right">
                        <ul class="social-icons">
                            <li><a href="https://www.facebook.com/jairograbiel.melojimenez/" target="_blank" title="Facebook"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="https://twitter.com/JairoMe73301708" target="_blank" title="Twitter"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="https://www.linkedin.com/feed/" target="blank" title="Linkedin"><i class="fa fa-linkedin"></i></a></li>
                            <li><a href="https://www.instagram.com/jairogabriel/" target="_blank" title="Instagram"><i class="fa fa-instagram"></i></a></li>
                        </ul>
                    </div>
                </div>*@
                <!-- top navigation -->
                <div class="top_nav">

                    <div class="container">
                        <nav class="navbar navbar-expand-sm navbar-light bg-light">
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-sm-12">
                                        @Html.Partial("_LoginPartial")
                                    </div>
                                    
                                </div>
                            </div>
                        </nav>
                    </div>
                </div>
              
                <!-- /top navigation -->
                <!--Page content-->
                <div class="right_col" role="main">
                    @RenderBody()
                    <br />
                    <br />
                </div>
                <!--Page content-->
            </div>
        </div>
        <br />
        <br />
        <br />
    </div>

    <!--Scripts-->
    @RenderSection("scripts", required: false)
    <script src="~/Vendors/jquery/jquery.min.js"></script>
    <script src="~/Content/Alela/js/custom.js"></script>
    <script src="~/Vendors/bootstrap/dist/js/bootstrap.bundle.js"></script>
    <script src="~/Vendors/nprogress/nprogress.js"></script>
    <script src="~/Content/myCss/Style.js"></script>
    <script src="~/Content/CreativeButtons/js/classie.js"></script>
    <script src="~/Content/CreativeButtons/js/modernizr.custom.js"></script>
